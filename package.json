{
  "name": "node-readme",
  "version": "0.1.7",
  "description": "Generate your JS project README.md using an ES6 template",
  "license": "MIT",
  "repository": "revolunet/node-readme",
  "author": {
    "name": "Julien Bouquillon",
    "email": "julien@bouquillon.com",
    "url": "http://github.com/revolunet"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "main": "src/index.js",
  "bin": {
    "node-readme": "bin/node-readme.js"
  },
  "scripts": {
    "readme": "node bin/node-readme.js",
    "build": "babel -d dist/ src/ && cp ./src/.README.md ./dist/.README.md"
  },
  "keywords": [
    "node",
    "readme",
    "doc",
    "babel"
  ],
  "browserify": {
    "transform": "babelify"
  },
  "testling": {
    "files": "spec/**/*.spec.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/5..latest",
      "ipad/5..latest",
      "android-browser/2..latest"
    ]
  },
  "dependencies": {
    "es6-template-strings": "1.0.0",
    "es6-templates": "0.2.2",
    "oss-license-name-to-url": "1.2.1"
  },
  "devDependencies": {
    "babel": "5.6.14",
    "babel-eslint": "3.1.23"
  }
}
